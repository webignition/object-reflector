{
    "name": "webignition/object-reflector",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/webignition/object-reflector",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\ObjectReflector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\ObjectReflector\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR12",
        "static-analysis": "./vendor/bin/phpstan analyse src tests --level=7",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@composer cs",
            "@composer static-analysis",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12.28"
    }
}
